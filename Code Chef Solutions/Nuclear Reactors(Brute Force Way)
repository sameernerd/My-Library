	public static void solve(String s){
		
	}
	public static void main(String[] args) throws IOException{
		long a=in.nextLong();
		long n=in.nextLong();
		int k=in.nextInt();
		long ar[]=new long[k];
		for(int i=1;i<=a;i++){
			for(int j=0;j<k;j++){
					if(ar[j]+1<n+1){
						ar[j]+=1;
						break;
					}
					else{
						ar[j]=0;
					}
			}
		}
		for(int j=0;j<k;j++)
			out.print(ar[j]+" ");
	}
}
  
